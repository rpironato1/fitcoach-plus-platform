{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8030",
    "dev:open": "vite --port 8030 --open",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:analyze": "vite build && npx vite-bundle-analyzer dist",
    "lint": "eslint .",
    "preview": "vite preview --port 8031",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui --port 8033",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "test:rls": "pg_prove tests/rls/*.sql --host localhost --port 54322 --dbname postgres --username postgres",
    "test:load": "k6 run tests/load/load-test.js",
    "test:stress": "k6 run tests/load/stress-test.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui-port=8035",
    "test:all": "npm run typecheck && npm run test:coverage",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "clean": "rm -rf dist node_modules/.vite",
    "audit": "npm audit",
    "outdated": "npm outdated",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lighthouse": "npm run build && npm run preview & sleep 5 && npx lighthouse http://localhost:8031 --output=html --output-path=./lighthouse-production.html --chrome-flags=\"--headless --no-sandbox\" && kill %1",
    "lighthouse:dev": "npx lighthouse http://localhost:8030 --output=html --output-path=./lighthouse-dev.html --chrome-flags=\"--headless --no-sandbox\"",
    "lighthouse:storybook": "npx lighthouse http://localhost:6006 --output=html --output-path=./lighthouse-storybook.html --chrome-flags=\"--headless --no-sandbox\"",
    "lighthouse:all": "npm run lighthouse && npm run lighthouse:storybook",
    "lighthouse:accessibility": "npm run build && npm run preview & sleep 5 && npx lighthouse http://localhost:8031 --output=html --output=json --output-path=./lighthouse-accessibility --chrome-flags=\"--headless --no-sandbox\" --preset=desktop --only-categories=accessibility && kill %1",
    "lighthouse:performance": "npm run build && npm run preview & sleep 5 && npx lighthouse http://localhost:8031 --output=html --output=json --output-path=./lighthouse-performance --chrome-flags=\"--headless --no-sandbox\" --preset=perf && kill %1",
    "chromatic": "npx chromatic --project-token=chpt_your_token_here",
    "test:a11y": "npm run lighthouse:accessibility && npm run lighthouse:storybook",
    "test:contrast": "npm run storybook & sleep 10 && npx lighthouse http://localhost:6006 --output=json --output-path=./contrast-test.json --chrome-flags=\"--headless --no-sandbox\" --only-audits=color-contrast && kill %1"
  },
